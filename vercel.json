{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/treblle-logo.svg",
      "dest": "/public/treblle-logo.svg"
    },
    {
      "src": "/treblle-favicon.png",
      "dest": "/public/treblle-favicon.png"
    },
    {
      "src": "/treblle-roi-preview.png",
      "dest": "/public/treblle-roi-preview.png"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
